#include <iostream>
#include <string>
using namespace std;

int main() {
    setlocale(LC_ALL, "RU");
    int num;
    cin >> num;
    string res;
    for (int d=1; d<=num; d++) {
        for (int i=1; i <=d; i++) {
            cout << i;
        }
        
        cout << endl;
    }
    for (int d= num-1; d >= 0; d--) {
        for (int i =1 ; i <= d; i++) {
            cout << i;

        }
        cout << endl;
    }
    
    return 0;
}
